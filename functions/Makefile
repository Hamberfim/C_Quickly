# makefile for functions dir

declareFunc: declareFunc.c
	gcc -o declareFunc.exe declareFunc.c

withArgs: withArgs.c
	gcc -o withArg.exe withArgs.c

recursiveFuncs: recursiveFuncs.c
	gcc -o recursiveFuncs.exe recursiveFuncs.c

useCustHead: useCustHead.c
	gcc -o useCustHead.exe useCustHead.c

menu: restrictStatic.c
	gcc -o menu.exe restrictStatic.c restrictAcces.c